{
  "name": "swan",
  "version": "2.0.0-alpha-4",
  "description": "Bot discord de Skript-MC.",
  "main": "./src/main.ts",
  "scripts": {
    "build": "tsc && cp -r ./assets ./build",
    "exec": "node -r source-map-support/register ./build/src/main.js",
    "start": "npm run build && cross-env NODE_ENV=production npm run exec -- --no-color",
    "dev": "npm run build && cross-env NODE_ENV=development npm run exec",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "patch": "npx patch-package",
    "postinstall": "npm run patch"
  },
  "authors": [
    "noftaly (noftaly#0359)",
    "Romitou (Romitou#9685)"
  ],
  "contributors": [
    "Olyno (Olyno#1234)",
    "WeeskyBDW (WeeskyBDW#6172)",
    "iTrooz_ (iTrooz_#2050)"
  ],
  "license": "GPL-3.0-only",
  "engines": {
    "node": ">=12.0.0"
  },
  "homepage": "https://github.com/Skript-MC/Swan#readme",
  "bugs": {
    "url": "https://github.com/Skript-MC/Swan/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Skript-MC/Swan.git"
  },
  "dependencies": {
    "@sentry/integrations": "^5.29.2",
    "@sentry/node": "^5.29.2",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "common-tags": "^1.8.0",
    "discord-akairo": "^8.1.0",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "moment": "^2.29.1",
    "mongoose": "^5.11.9",
    "mongoose-autopopulate": "^0.12.3",
    "nanoid": "^3.1.20",
    "node-cron": "^2.0.3",
    "patch-package": "^6.2.2",
    "pupa": "^2.1.1",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "@types/node": "^14.14.19",
    "@types/node-cron": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.17.0",
    "eslint-config-noftalint": "^1.17.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^25.0.1",
    "typescript": "^4.1.3"
  }
}
