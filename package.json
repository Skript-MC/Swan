{
  "name": "swan",
  "version": "2.0.0",
  "description": "Bot discord de Skript-MC.",
  "main": "./src/main.ts",
  "scripts": {
    "build": "rm -rf ./build && tsc && cp -r ./assets/ ./build/assets",
    "build:win": "rd /s /q \"build\" && tsc && xcopy .\\assets\\ .\\build\\assets\\ /D",
    "exec": "node --no-warnings ./build/src/main.js",
    "start": "npm run build && NODE_ENV=production npm run exec -- --no-color",
    "start:win": "npm run build:win && set NODE_ENV=production && npm run exec -- --no-color",
    "dev": "npm run build && NODE_ENV=development npm run exec",
    "dev:win": "npm run build:win && set NODE_ENV=development && npm run exec",
    "tools:update-databases": "npm run build && node ./build/tools/update-databases.js",
    "tools:update-databases:win": "npm run build:win && node ./build/tools/update-databases.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "patch": "npx patch-package",
    "postinstall": "npm run patch"
  },
  "authors": [
    "noftaly (noftaly#0359)",
    "Romitou (Romitou#9685)"
  ],
  "contributors": [
    "Olyno (Olyno#1234)",
    "WeeskyBDW (WeeskyBDW#6172)",
    "Aless (Aless#6161)",
    "iTrooz_ (iTrooz_#2050)"
  ],
  "license": "GPL-3.0-only",
  "engines": {
    "node": ">=12.0.0"
  },
  "homepage": "https://github.com/Skript-MC/Swan#readme",
  "bugs": {
    "url": "https://github.com/Skript-MC/Swan/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Skript-MC/Swan.git"
  },
  "dependencies": {
    "@octokit/rest": "^18.5.2",
    "@octokit/types": "^6.13.0",
    "@sentry/integrations": "^6.2.3",
    "@sentry/node": "^6.2.3",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "common-tags": "^1.8.0",
    "discord-akairo": "^8.1.0",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "he": "^1.2.0",
    "jaro-winkler": "^0.2.8",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "mongoose": "^5.12.2",
    "mongoose-autopopulate": "^0.12.3",
    "nanoid": "^3.1.22",
    "node-cron": "^3.0.0",
    "node-emoji": "^1.10.0",
    "patch-package": "^6.4.7",
    "pupa": "^2.1.1",
    "semver": "^7.3.5",
    "source-map-support": "^0.5.19",
    "turndown": "^7.0.0"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "@types/he": "^1.1.1",
    "@types/mongoose-autopopulate": "^0.10.1",
    "@types/node": "^14.14.37",
    "@types/node-cron": "^2.0.3",
    "@types/node-emoji": "^1.8.1",
    "@types/semver": "^7.3.4",
    "@types/turndown": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.23.0",
    "eslint-config-noftalint": "^1.21.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^29.0.0",
    "typescript": "^4.2.3"
  },
  "_moduleAliases": {
    "@/app": "./build/src",
    "@/conf": "./build/config",
    "@/root": "./build/"
  }
}
