{
  "name": "swan",
  "version": "3.2.0",
  "description": "Bot discord de Skript-MC.",
  "main": "dist/src/main.js",
  "type": "module",
  "imports": {
    "#app/*": "./dist/src/*.js",
    "#config/*": "./dist/config/*.js",
    "#models/*": "./dist/src/models/*.js",
    "#moderation/*": "./dist/src/moderation/*.js",
    "#resolvers/*": "./dist/src/resolvers/*.js",
    "#structures/*": "./dist/src/structures/*.js",
    "#types/*": "./dist/src/types/*.js",
    "#utils/*": "./dist/src/utils/*.js"
  },
  "scripts": {
    "clean": "rimraf dist/",
    "build": "tsup",
    "watch": "tsup --watch",
    "start": "node --enable-source-maps ./dist/src/main.js",
    "dev": "tsup --watch --onSuccess \"npm start\"",
    "typecheck": "tsc -p tsconfig.base.json",
    "tools:update-databases": "node tools/update-databases.js",
    "tools:remove-commands": "node tools/remove-commands.js",
    "lint": "biome lint .",
    "lint:fix": "biome lint --apply .",
    "check": "biome check .",
    "check:fix": "biome check --apply ."
  },
  "authors": ["noftaly (@noftaly)", "Romitou (@romitou)"],
  "contributors": [
    "Olyno (@olyno)",
    "WeeskyBDW (@weeskybdw)",
    "Aless (@alessevan)",
    "Skylyxx (@skylyxx)",
    "iTrooz_ (@itrooz)"
  ],
  "license": "GPL-3.0-only",
  "engines": {
    "node": ">=18.0.0"
  },
  "homepage": "https://github.com/Skript-MC/Swan#readme",
  "bugs": {
    "url": "https://github.com/Skript-MC/Swan/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Skript-MC/Swan.git"
  },
  "dependencies": {
    "@octokit/rest": "^20.1.0",
    "@octokit/types": "^13.4.0",
    "@sapphire/decorators": "^6.1.0",
    "@sapphire/discord-utilities": "^3.2.3",
    "@sapphire/discord.js-utilities": "^7.1.6",
    "@sapphire/framework": "^5.1.0",
    "@sapphire/pieces": "^4.2.2",
    "@sapphire/plugin-logger": "^4.0.2",
    "@sapphire/utilities": "^3.15.3",
    "@sentry/node": "^7.110.0",
    "axios": "^1.6.8",
    "common-tags": "^1.8.2",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "fastest-levenshtein": "^1.0.16",
    "he": "^1.2.0",
    "moment": "^2.30.1",
    "mongoose": "^8.3.1",
    "mongoose-autopopulate": "^1.1.0",
    "nanoid": "^5.0.7",
    "node-cron": "^3.0.3",
    "node-emoji": "^2.1.3",
    "pupa": "^3.1.0",
    "reflect-metadata": "^0.2.2",
    "semver": "^7.6.0",
    "turndown": "^7.1.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.1",
    "@sapphire/ts-config": "^5.0.0",
    "@types/common-tags": "^1.8.4",
    "@types/he": "^1.2.3",
    "@types/node": "^20.12.7",
    "@types/node-cron": "^3.0.11",
    "@types/semver": "^7.5.8",
    "@types/turndown": "^5.0.4",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  }
}
