{
  "name": "swan",
  "version": "2.0.0-alpha-5",
  "description": "Bot discord de Skript-MC.",
  "main": "./src/main.ts",
  "scripts": {
    "build": "rm -rf ./build && tsc && cpy --parents ./assets/ ./build/",
    "exec": "node -r source-map-support/register -r module-alias/register ./build/src/main.js",
    "start": "npm run build && cross-env NODE_ENV=production npm run exec -- --no-color",
    "dev": "npm run build && cross-env NODE_ENV=development npm run exec",
    "tools:update-databases": "npm run build && node ./build/src/tools/update-databases.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "patch": "npx patch-package",
    "postinstall": "npm run patch"
  },
  "authors": [
    "noftaly (noftaly#0359)",
    "Romitou (Romitou#9685)"
  ],
  "contributors": [
    "Olyno (Olyno#1234)",
    "WeeskyBDW (WeeskyBDW#6172)",
    "iTrooz_ (iTrooz_#2050)"
  ],
  "license": "GPL-3.0-only",
  "engines": {
    "node": ">=12.0.0"
  },
  "homepage": "https://github.com/Skript-MC/Swan#readme",
  "bugs": {
    "url": "https://github.com/Skript-MC/Swan/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Skript-MC/Swan.git"
  },
  "dependencies": {
    "@octokit/rest": "^18.0.15",
    "@octokit/types": "^6.6.0",
    "@sentry/integrations": "^6.0.3",
    "@sentry/node": "^6.0.3",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "common-tags": "^1.8.0",
    "cpy-cli": "^3.1.1",
    "discord-akairo": "^8.1.0",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "he": "^1.2.0",
    "jaro-winkler": "^0.2.8",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "mongoose": "^5.11.14",
    "mongoose-autopopulate": "^0.12.3",
    "nanoid": "^3.1.20",
    "node-cron": "^2.0.3",
    "patch-package": "^6.2.2",
    "pupa": "^2.1.1",
    "semver": "^7.3.4",
    "source-map-support": "^0.5.19",
    "turndown": "^7.0.0"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "@types/he": "^1.1.1",
    "@types/mongoose-autopopulate": "^0.10.1",
    "@types/node": "^14.14.22",
    "@types/node-cron": "^2.0.3",
    "@types/semver": "^7.3.4",
    "@types/turndown": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.19.0",
    "eslint-config-noftalint": "^1.19.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^27.0.0",
    "typescript": "^4.1.3"
  },
  "_moduleAliases": {
    "@/app": "./build/src",
    "@/conf": "./build/config",
    "@/root": "./build/"
  }
}
